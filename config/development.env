# Configurações do Ambiente de Desenvolvimento
# Copie este arquivo para .env e configure as variáveis

# Configurações do Banco de Dados
DATABASE_URL="postgresql://postgres:postgres@localhost:5432/catolica_portfolio"
DIRECT_URL="postgresql://postgres:postgres@localhost:5432/catolica_portfolio"

# Configurações do Clerk (Autenticação)
# Obtenha essas chaves em: https://clerk.com
CLERK_SECRET_KEY="sk_test_your_clerk_secret_key_here"
CLERK_PUBLISHABLE_KEY="pk_test_your_clerk_publishable_key_here"

# Configurações do Stripe (Pagamentos)
# Obtenha essas chaves em: https://stripe.com
STRIPE_SECRET_KEY="sk_test_your_stripe_secret_key_here"
STRIPE_PUBLISHABLE_KEY="pk_test_your_stripe_publishable_key_here"

# Configurações do SonarCloud
# Obtenha o token em: https://sonarcloud.io
SONAR_TOKEN="your_sonar_token_here"

# Configurações do Ambiente
NODE_ENV="development"
PORT=3000

# Configurações do Frontend
VITE_CLERK_PUBLISHABLE_KEY="pk_test_your_clerk_publishable_key_here"
VITE_STRIPE_PUBLISHABLE_KEY="pk_test_your_stripe_publishable_key_here"
VITE_API_URL="http://localhost:3000"
