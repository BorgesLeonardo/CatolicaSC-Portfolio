{
  "name": "catolica-sc-portfolio-backend",
  "version": "1.0.0",
  "description": "Backend API for Catolica SC Portfolio Crowdfunding Platform",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc -p tsconfig.build.json",
    "start": "node dist/server.js",
    "lint": "eslint . --ext .ts",
    "typecheck": "tsc --noEmit",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:api": "node tests/test-api.js",
    "prisma:studio": "prisma studio",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy"
  },
  "dependencies": {
    "@clerk/express": "^1.0.0",
    "@prisma/client": "^5.7.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "stripe": "^18.5.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.5",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "mockdate": "^3.0.5",
    "prisma": "^5.7.1",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.5",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "crowdfunding",
    "api",
    "backend",
    "express",
    "prisma",
    "clerk"
  ],
  "author": "Catolica SC Portfolio Team",
  "license": "MIT"
}
